main::IO()
comissao::Double->Double
comissao ved
  |ved <10000 = (ved*0.03)
  |ved >=10000 && ved<= 50000 = (ved*0.04)
  |otherwise = (ved*0.05)
  
main = do
putStrLn ("quanto o vendedor vendeu? ")
ved<-readLn

putStrLn ("O pagamento do vendedor Ã©:  "++show(comissao ved))


